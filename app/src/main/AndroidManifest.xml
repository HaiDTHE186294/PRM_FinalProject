        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher2_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.DayNight.NoActionBar">
        <activity
            android:name=".ui.inventory.ItemDetailActivity"
            android:exported="false" />
        <activity
            android:name=".ui.viewlog.ViewLogDetailActivity"
            android:exported="false" />
        <activity
            android:name=".ui.addlog.AddLogActivity"
            android:exported="false" />
        <activity
            android:name=".ui.project.projectmanage.ProjectActivity"
