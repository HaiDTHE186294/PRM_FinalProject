            android:name=".ui.sds.SdsLookupActivity"
            android:exported="false" />
        <activity
            android:name=".ui.equipment.PdfViewerActivity"
            android:exported="false" />
        <activity
            android:name=".ui.equipment.EquipmentDetailActivity"
            android:exported="false" />
        <activity
            android:name=".ui.experimentInfo.ExperimentInfoActivity"
            android:exported="false" />
        <activity
            android:name=".ui.experimentdetail.ExperimentDetailActivity"
            android:exported="false" />
        <activity
            android:name=".ui.loginmaindashboard.maindashboard.MainDashboardActivity"
            android:exported="false" />
        <activity
            android:name=".ui.loginmaindashboard.login.SystemLoginActivity"
